sizes="10 20 40 80 160 320 640 1280 2560"
asizes="1 2 3 4 8 16"

allsizes() {
for i in $sizes
do for j in $asizes
do ./Effect $1 -s${i} -w${j} >> effects/$2
done
done
}

allsizes -v prom
allsizes -l linear-l
allsizes -k kata-k
allsizes -q fuse-q
allsizes -p press-p
allsizes "-*" "stellate-*"
allsizes -c semcat-c
allsizes -y syncat-y
